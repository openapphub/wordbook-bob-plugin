    {
      "identifier": "word_only",
      "type": "menu",
      "title": "仅保存单词",
      "defaultValue": "1",
      "menuValues": [
        {
          "title": "是",
          "value": "1"
        },
        {
          "title": "否",
          "value": "0"
        }
      ]
    },
    {
      "identifier": "wordbook_id",
      "type": "text",
      "title": "欧路单词本 id"
